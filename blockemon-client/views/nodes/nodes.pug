extends ../layout

block content
  h1 Nodes
  form(action='/nodes/add', method='GET')
    button.btn.btn.btn-primary(type='submit')
      i.fa.fa-pencil
      | Add
  table.table
    thead
      tr
        th User
        th HTTP URL
        th P2P URL
    tbody
      each n in nodes
        tr
          td= n.user 
          td= n.http
          td= n.p2p
